<?xml version="1.0" encoding="utf-8"?>
<easysql-mapping>

	<!-- 数据库类型 mysql sqlserver oracle -->
	<database class="mysql" />

	<!-- 表主键类型 native assigned identity sequence-->
	<generator class="native">
		<param value="SEQ_USER" />
	</generator>
 
	<!-- 列名规则 hump segmentation -->
	<field-rule class="segmentation" />

	<!-- 实体类 -->
	<entitys>
		<entity class="examples.dhome.domain.User" />
		<entity class="examples.dhome.domain.Device" />
	</entitys>

</easysql-mapping>
