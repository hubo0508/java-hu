<?xml version="1.0" encoding="utf-8"?>
<easysql-mapping>

	<!-- 数据库类型 mysql sqlserver oracle -->
	<database class="oracle" />

	<!-- 表主键类型 native assigned identity sequence-->
	<generator class="sequence">
		<param value="SEQ_USER" />
	</generator>

	<!-- 列名规则 hump segmentation -->
	<field-rule class="segmentation" />

	<!-- 实体类 -->
	<entitys>
		<entity class="examples.domain.User" />
		<entity class="examples.domain.Account" />
		<entity class="examples.domain.Device" />
	</entitys>

</easysql-mapping>
